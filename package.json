{
    "name": "@nera-static/plugin-page-navigation",
    "version": "2.1.0",
    "description": "A plugin for Nera static site generator to create sibling page navigation within directories. Perfect for section-based navigation and multi-page document structures.",
    "main": "index.js",
    "scripts": {
        "publish-template": "node ./bin/publish-template.js",
        "lint": "eslint .",
        "test": "vitest",
        "test:clean": "node ./test/teardown.js"
    },
    "type": "module",
    "exports": {
        ".": "./index.js"
    },
    "bin": {
        "nera-page-navigation": "./bin/publish-template.js"
    },
    "files": [
        "bin",
        "config",
        "views",
        "index.js"
    ],
    "keywords": [
        "nera",
        "plugin",
        "page-navigation",
        "sibling-navigation",
        "static-site",
        "static-site-generator",
        "directory-navigation",
        "navigation",
        "template"
    ],
    "author": "Michael Becker",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "https://github.com/seebaermichi/nera-plugin-page-navigation"
    },
    "bugs": {
        "url": "https://github.com/seebaermichi/nera-plugin-page-navigation/issues"
    },
    "homepage": "https://github.com/seebaermichi/nera-plugin-page-navigation#readme",
    "dependencies": {
        "@nera-static/plugin-utils": "^1.1.0"
    },
    "engines": {
        "node": ">=18"
    },
    "devDependencies": {
        "cheerio": "^1.1.0",
        "eslint": "^9.31.0",
        "husky": "^9.1.7",
        "pug": "^3.0.3",
        "vitest": "^3.2.4"
    },
    "sideEffects": false
}
