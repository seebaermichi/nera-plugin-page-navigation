{
    "name": "@nera-static/plugin-page-navigation",
    "version": "2.0.0",
    "description": "A plugin for static side generator nera to create a page navigation.",
    "main": "index.js",
    "scripts": {
        "publish-template": "node ./bin/publish-template.js",
        "lint": "eslint .",
        "test": "vitest",
        "test:clean": "node ./test/teardown.js"
